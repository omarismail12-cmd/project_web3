{
  "version": 2,
  "builds": [
    { "src": "api/**/*.php", "use": "vercel-php@0.6.2" },
    { "src": "**/*.php", "use": "vercel-php@0.6.2" },
    { "src": "**/*.html", "use": "@vercel/static" },
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
