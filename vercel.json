{
  "version": 2,
  "builds": [
    { "src": "api/**/*.php", "use": "vercel-php@0.6.2" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/includes/(.*)", "dest": "/includes/$1" },
    { "src": "/pages/(.*)", "dest": "/pages/$1" },
    { "src": "/([^/]+\\.html)", "dest": "/pages/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/pages/home.html" }
  ]
}
